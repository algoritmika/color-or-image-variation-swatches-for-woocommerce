{
	"name": "algoritmika/appealing-variation-for-woocommerce",
	"description": "Provides an appealing variation instead of the native boring one from WooCommerce",
	"keywords": [
		"wordpress",
		"plugin",
		"woocommerce",
		"variation",
		"term",
		"taxonomy"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Tomas Anbinderis",
			"email": "anbinderis@gmail.com"
		},
		{
			"name": "Pablo Pacheco",
			"email": "pablo.sg.pacheco@gmail.com"
		}
	],
	"config": {
		"preferred-install": "dist"
	},
	"type": "wordpress-plugin",
	"require": {
		"php": ">=5.3.2",
		"composer/installers": ">=v1.2.0",
		"webdevstudios/cmb2": "dev-master"
	},
	"autoload": {
		"files": [
			"vendor/cmb2/init.php"
		]
	},
	"extra": {
		"installer-paths": {
			"vendor/{$name}/": [
				"webdevstudios/cmb2"
			]
		}
	}
}